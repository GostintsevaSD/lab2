        "# Задание task_02_20.\n",
        "#\n",
        "# Выполнил: Гостинцева С.Д.\n",
        "# Группа: ЦИБ-241\n",
        "\n",
        "n = int(input())\n",
        "print(f\"n = {n}\")\n",
        "\n",
        "for num in range(100, 1000):\n",
        "    a = num // 100\n",
        "    b = (num // 10) % 10\n",
        "    c = num % 10\n",
        "    if a + b + c == n:\n",
        "        print(num, end=' ')"
